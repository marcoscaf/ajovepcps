<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>membro</from-outcome>
			<to-view-id>/pages/member/member.xhtml</to-view-id>
		</navigation-case></navigation-rule>


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>grupo</from-outcome>
			<to-view-id>/pages/group/group.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
		<application>
		<locale-config>
			<default-locale>pt</default-locale>
			<supported-locale>pt</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>br.com.ajovepcps.i18n.messages</base-name>
			<var>bundle</var>
		</resource-bundle>
	</application>


</faces-config>
